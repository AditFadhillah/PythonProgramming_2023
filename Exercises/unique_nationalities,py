def unique_items(lst):
    unique_list = []
    for item in lst:
        if item not in unique_list:
            unique_list.append(item)
    return unique_list

if __name__ == "__main__":
    nationalities = [
        "United States",
        "Brazil",
        "Germany",
        "Spain",
        "The Netherlands",
        "United States",
        "United States",
        "Australia",
        "Japan",
        "Egypt",
        "South-Africa",
        "South-Korea",
        "China",
        "Japan",
        "Mexico",
        "Germany",
        "Sweden",
        "The Netherlands",
        "Canada"
    ]

    unique_nationalities = unique_items(nationalities)
    print(unique_nationalities)
